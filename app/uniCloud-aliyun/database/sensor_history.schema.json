// 文档教程: https://uniapp.dcloud.net.cn/uniCloud/schema
{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": true,
		"create": true,
		"update": true,
		"delete": true
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"deviceId": {
			"bsonType": "string",
			"description": "关联设备ID"
		},
		"sensorId": {
			"bsonType": "string",
			"description": "传感器ID"
		},
		"value": {
			"bsonType": ["int", "string", "bool","double"],
			"description": "传感器数值"
		},
		"timestamp": {
			"bsonType": "timestamp",
			"description": "记录时间戳"
		}
	}
}